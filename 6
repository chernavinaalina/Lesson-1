my_dict = {'Tony' : 1998, 'Mark' : 2006, 'Alice' : 1996, 'Maria' : 2002}
print(my_dict)
print(my_dict['Tony'])
print(my_dict.get('Mary'))
my_dict.update({'Mary' : 1998, 'Nick': 1987})
name_deleted = my_dict.pop('Tony')
print(my_dict)
print(name_deleted)

my_set = {1, 2, 3, 1, 2, 3, 'Alina', 'Lisa', 'Mom', 'Alina', 3.14, 5.15, 3.14}
print(my_set)
my_set.update({10, 15, 'Lolla', 7.14})
my_set.remove(2)
print(my_set)
